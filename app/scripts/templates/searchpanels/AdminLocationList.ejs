<div class="shadow panel panel-locations">
	<div class="navbar"><em id="q">Edit a location.</em> </div>
	<div class="bd spacing-full">

		<h3>Campus: ${data.name}</h3>

		<h4>Map: ${data.map.name }</h4> 

		<ul class="list list-striped">
			<% _.each(data.map.locations, function(loc) { %> 
			    <li id="<%= loc.locationid %>" <% if (loc.selected) { %>class="active"<% } %>><a class="location" href="#${loc.name}" >${loc.name}</a> <button data-campusmap="details:<%= loc.locationid %>,cmd:AdminLocation" class="locations">View details</button></li>
			<% }); %>
			<% if (data.map.locations.length === 0) { %><li>No locations yet.</li><% } %>
		</ul>
	</div>
</div>

