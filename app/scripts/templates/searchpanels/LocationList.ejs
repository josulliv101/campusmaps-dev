<div class="shadow panel">
	<div class="bd">
		<button class="back" onclick="window.history.back()">back</button>
		<h3>Campus: ${data.name}</h3>

		<h4>Map: ${data.map.name }</h4> 

		<ul>
			<% _.each(data.map.locations, function(loc) { %> 
			    <li><a href="#">${loc.name}</a></li>
			<% }); %>
			<% if (data.map.locations.length === 0) { %><li>No locations yet.</li><% } %>
		</ul>
	</div>
</div>