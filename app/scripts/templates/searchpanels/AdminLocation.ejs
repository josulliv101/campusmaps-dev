<div class="shadow panel admin">
	<div class="navbar">Edit this location</div>
	<div class="bd spacing">
		<h3>Admin Location</h3>
		<form class="form-horizontal" role="form">
		  <div class="form-group">
		    <label for="inputId" class="col-sm-4 control-label">Location Id</label>
		    <div class="col-sm-8">
		      <input class="form-control" id="inputId" placeholder="Location Id" value="${data.locationid}">
		    </div>
		  </div>
		  <div class="form-group">
		    <label for="inputName" class="col-sm-4 control-label">Name</label>
		    <div class="col-sm-8">
		      <input class="form-control" id="inputName" placeholder="Location Name" value="${data.name}">
		    </div>
		  </div>
		  <div class="form-group">
		    <label for="inputEmphasis" class="col-sm-4 control-label">Emphasis</label>
		    <div class="col-sm-8">
		      <input class="form-control" id="inputEmphasis" placeholder="Emphasis" value="${data.emphasis}">
		    </div>
		  </div>
		  <div class="form-group">
		    <label for="inputLatLng" class="col-sm-4 control-label">LatLng</label>
		    <div class="col-sm-8">
		      <input class="form-control" id="inputLatLng" placeholder="Latitude/Longitude" value="${data.latlng}">
		    </div>
		  </div>
		  <div class="form-group">
		    <label for="inputLabelPlacement" class="col-sm-4 control-label">Label Placement</label>
		    <div class="col-sm-8">
		      <input class="form-control" id="inputLabelPlacement" placeholder="" value="${data.labelplacement}">
		    </div>
		  </div>
		  <div class="form-group">
		    <label for="inputTags" class="col-sm-4 control-label">Tags</label>
		    <div class="col-sm-8">
		      <input class="form-control" id="inputTags" placeholder="" value="${data.tags}">
		    </div>
		  </div>
		  <div class="form-group">
		    <label for="inputImage" class="col-sm-4 control-label">Image</label>
		    <div class="col-sm-8">
		      <input class="form-control" id="inputImage" placeholder="Image Name" value="${data.imageurl}">
		    </div>
		  </div>
		  <div class="form-group">
		    <div class="col-sm-offset-2 col-sm-8">
		      <button type="submit" class="btn btn-default btn-save">Save</button>
		    </div>
		  </div>
		</form>
	</div>
</div>
